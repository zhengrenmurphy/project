 Button -->\n        <button class=\"edit-timeslot\">Edit timeslot time</button>\n    </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport { eventBus } from \"../main\";\nexport default{\n    name: \"res-business-capacity\",\n    props: {\n        capacity: Object\n    },\n    data(){\n        return{\n            // capacity:{\n            //     name: \"sss\",\n            // }\n        }\n    },\n    methods: {\n        Contact: function(name) {\n            axios.post(`/api/business/contact/${name}`, {})\n            .then((res) => {\n                // handle success\n                eventBus.$emit(\"contact-user-success\", res);\n            })\n            .catch(err => {\n                // handle error\n                eventBus.$emit(\"contact-user-error\", err);\n            })\n        }\n    }\n}\n</script>\n\n<style scoped>\n    div{\n        display: flex;\n        justify-content: space-around;\n        width: 80%;\n    }\n    .capacity-data{\n        border-top: 1px solid black;\n        padding: 3px;\n    }\n    .capacity-table{\n        border: 1px solid black;\n        border-radius: 6px;\n        border-spacing: 0px;\n        width: 50%;\n    }\n    .capacity-data-container{\n        display: flex;\n        justify-content: space-between;\n        align-items: center;\n    }\n    .contact{\n        background-color: #F5C19E;\n        border-radius: 4px;\n        border-width: 0px;\n    }\n    .edit-timeslot{\n        background-color: #F5C19E;\n        border-radius: 4px;\n        border-width: 0px;\n        height: 10%;\n    }\n</style>"]}]}