nfo in bussInfos\"\n            v-bind:key=\"bussInfo.freetID\"\n            v-bind:bussInfo=\"bussInfo\"\n            />\n            <!-- </div> -->\n        </div>\n        \n        <GmapMap\n             :center=\"mapCoordinates\"\n             \n             :zoom=\"16\"\n             style=\"width:640px;height:360px;margin:32px auto;\"\n             ref=\"mapRef\"\n        ></GmapMap>\n        <div>\n            <div >\n            <h1> Your coordinates: </h1>\n            <p>{{coordinates.lat}} Latitude, {{coordinates.lng}} Longitude</p>\n            </div>\n\n            <div>\n            <h1> Map coordinates: </h1>\n            <p>{{mapCoordinates.lat}} Latitude, {{mapCoordinates.lng}} Longitude</p>\n            </div>\n        \n        </div>\n    </div>\n    \n</template>\n\n<script>\n// import axios from \"axios\";\n\nimport BussInfo from \"./BussInfo\";\n\nimport { eventBus } from \"../main\";\n\nexport default {\n  name: \"BussInfoList\",\n\n  components:{BussInfo},\n\n  props: {\n\n  },\n\n  data() {\n      return {\n\n            bussInfos: [],\n            map:null,\n            coordinates: {\n                \n                lat: 0,\n                lng: 0\n            },\n\n      }\n  },\n\n  created: function() {\n      eventBus.$on(\"get-location\", (res) => {\n        //   this.map.lat = 3;\n        //   this.map.lng = 55;\n        this.map = {lat: res.lat,\n                    lng: res.lng}\n        //   alert(this.map.lat);\n\n      });\n      \n      this.$getLocation({})\n        .then(coordinates => {\n            this.coordinates = coordinates;\n        })\n        .catch(error => alert(error));\n\n  },\n\n  mounted: function() {\n      this.loadBussInfo();\n    //   this.$refs.mapRef.$mapPromise.then(map=>this.map = map);\n\n\n  },\n\n  methods: {\n      loadBussInfo: function() {\n          this.bussInfos = [\n              {index: 0,\n                name: \"1369 Coffee House\",\n                takeOut: true,\n                delievery: true,\n                outdoor: false,\n                indoor: false,\n                lat:42.373760,\n                lng:-71.118800,\n                address: \"757 Mass Ave\",\n                update:\"Here is the massage for Covid updating!\"},\n                {name: \"2 4-1 Optical\",\n                index: 1,\n                takeOut: false,\n                delievery: false,\n                outdoor: false,\n                indoor: false,\n                lat:42.370900,\n                lng:-71.114060,\n                address: \"1128 Massachusetts Ave\",\n                update:\"Here is the massage for Covid updating!\"},\n                {name: \"5th Street Laundromat\",\n                index: 2,\n                lat:42.357440,\n                lng:-71.092670,\n                takeOut: false,\n                delievery: false,\n                outdoor: false,\n                indoor: false,\n                address: \"58 5th Street\",\n                update:\"Here is the massage for Covid updating!\"},\n                {name: \"730 Tavern\",\n                index: 3,\n                lat:42.365930,\n                lng:-71.105140,\n                takeOut: true,\n                delievery: true,\n                outdoor: true,\n                indoor: true,\n                address: \"730 Massachusetts Ave\",\n                update:\"Here is the massage for Covid updating!\"},\n                {name: \"9Zaab Thai Street Food\",\n                index: 4,\n                lat:42.358950,\n                lng:-71.093450,\n                takeOut: true,\n                delievery: true,\n                outdoor: false,\n                indoor: false,\n                address: \"569 Cambridge St\",\n                update:\"Here is the massage for Covid updating!\"},\n          ]\n\n      }\n\n    },\n    computed:{\n        mapCoordinates(){\n            if(!this.map) {\n                return {\n                    lat:0, \n                    lng:0\n                }\n            }\n            return{\n                lat:this.map.lat,\n                lng:this.map.lng\n            }\n        }\n    }\n\n\n};\n</script>"]}]}