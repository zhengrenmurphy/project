   hour: 'numeric',\n      minute: 'numeric',\n      second: 'numeric'\n      }\n    };\n  },\n  methods:{\n    cancelReservation: function(){\n      axios.delete(`/api/client/reservation/${this.res}`, {})\n      .then((res) => {\n          // handle success\n          eventBus.$emit(\"cancel-reservation-success\", res);\n        })\n        .catch(err => {\n          // handle error\n          eventBus.$emit(\"cancel-reservation-error\", err);\n        })\n        .then(this.$router.go(0));\n    }\n  }\n};\n</script>\n\n<style scoped>\nh1{\n    color: #f5c19e\n}\nbutton{\n  background-color: #93b1ff;\n  color: white;\n  border-radius: 4px;\n  border: none;\n  padding: 4px;\n  margin-left: 20px;\n}\n.res-date-container{\n    display: flex;\n    margin-bottom: 10px;\n}\n</style>"]}]}