AAA,KAAA,CACA,IADA,CACA,cADA,EACA,WADA,EAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA;AACA,UAAA,KAAA,CAAA,OAAA,GAAA,GAAA,CAAA,IAAA,CAAA,OAAA;AACA,SALA,EAMA,KANA,CAMA,UAAA,GAAA,EAAA;AACA;AACA,UAAA,KAAA,CAAA,KAAA,GAAA,GAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KAAA;AACA,SATA,EAUA,IAVA,CAUA,YAAA;AACA;AACA,UAAA,KAAA,CAAA,SAAA;;AACA,UAAA,KAAA,CAAA,aAAA;AACA,SAdA;AAeA;AACA,KApCA;AAqCA,IAAA,SAAA,EAAA,qBAAA;AACA,WAAA,QAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,EAAA;AACA,WAAA,SAAA,GAAA,EAAA;AACA,WAAA,KAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,EAAA;AACA,KA3CA;AA4CA,IAAA,aAAA,EAAA,yBAAA;AAAA;;AACA,MAAA,UAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,EAAA;;AACA,YAAA,MAAA,CAAA,OAAA,IAAA,EAAA,EAAA;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,QAAA;;AACA,UAAA,MAAA,CAAA,OAAA,GAAA,EAAA;AACA;AACA,OANA,EAMA,IANA,CAAA;AAOA;AApDA;AAbA,CAAA","sourcesContent":["<template>\n  <div class=\"client-signup-form\">\n    <div class=\"client-signup-form-top\">\n      <input\n        class=\"name\"\n        style=\"margin-right: 20px\"\n        v-model=\"firstName\"\n        placeholder=\"First Name\"\n      />\n      <input class=\"name\" v-model=\"lastName\" placeholder=\"Last Name\" />\n    </div>\n\n    <input v-model=\"username\" placeholder=\"Username\" />\n    <input v-model=\"password\" placeholder=\"Password\" />\n    <input v-model=\"email\" placeholder=\"Email\" />\n\n    <div class=\"client-signup-form-buttons\">\n      <button class=\"submit\" v-on:click=\"signUp\">Submit</button>\n      <router-link to=\"/login\">\n        <button class=\"cancel\">Cancel</button>\n      </router-link>\n    </div>\n    <div class=\"error-message\">\n      {{ error }}\n    </div>\n    <div class=\"success-message\">\n      {{ success }}\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n  name: \"client-signup-form\",\n  data() {\n    return {\n      username: \"\",\n      password: \"\",\n      email: \"\",\n      firstName: \"\",\n      lastName: \"\",\n      error: \"\",\n      success: \"\",\n    };\n  },\n  methods: {\n    signUp: function () {\n      if (\n        this.username === \"\" ||\n        this.password === \"\" ||\n        this.email === \"\" ||\n        this.firstName === \"\" ||\n        this.lastName === \"\"\n      ) {\n        this.error = \"You need to fill up all the entries to sign up!\";\n        this.resetForm();\n        this.clearMessages();\n      } else {\n        const bodyContent = {\n          username: this.username,\n          password: this.password,\n          email: this.email,\n          firstName: this.firstName,\n          lastName: this.lastName,\n        };\n        axios\n          .post(\"/api/clients\", bodyContent)\n          .then((res) => {\n            // handle success\n            this.success = res.data.message;\n          })\n          .catch((err) => {\n            // handle error\n            this.error = err.response.data.error;\n          })\n          .then(() => {\n            // always executed\n            this.resetForm();\n            this.clearMessages();\n          });\n      }\n    },\n    resetForm: function () {\n      this.username = \"\";\n      this.password = \"\";\n      this.firstName = \"\";\n      this.email = \"\";\n      this.lastName = \"\";\n    },\n    clearMessages: function () {\n      setTimeout(() => {\n        this.error = \"\";\n        if (this.success != \"\") {\n          this.$router.push(\"/login\");\n          this.success = \"\";\n        }\n      }, 1000);\n    },\n  },\n};\n</script>\n\n<style scoped>\n.client-signup-form-buttons {\n  display: flex;\n  justify-content: center;\n}\nbutton {\n  margin: 0;\n}\n.submit {\n  margin-right: 30px;\n}\n\ninput {\n  border: 1px solid black;\n  width: 200px;\n  height: 30px;\n  border-radius: 4px;\n  margin-bottom: 20px;\n}\n.name {\n  width: 90px;\n}\n.client-signup-form {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n}\n</style>"],"sourceRoot":"src/components"}]}