KAAA,KAAA,CAAA,QANA;AAMA,QAAA,MAAA,EAAA,KAAA,KAAA,CAAA,QANA;AAMA,QAAA,OAAA,EAAA,KAAA,IAAA,CAAA,QANA;AAMA,QAAA,KAAA,EAAA,KAAA,IAAA,CAAA,QANA;AAOA,QAAA,GAAA,EAAA,KAAA;AAPA,OAAA;AASA,MAAA,KAAA,CAAA,GAAA,yBAAA,WAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,SAAA;AACA,QAAA,QAAA,CAAA,KAAA,CAAA,gBAAA,EAAA,IAAA;AACA,OALA,EAKA,KALA,CAKA,YAAA;AACA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,SAAA;AACA,QAAA,QAAA,CAAA,KAAA,CAAA,gBAAA,EAAA,IAAA;AACA;;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,SAAA;AACA,OAXA;AAaA,KAxCA;AAyCA,IAAA,aAAA,EAAA,yBAAA;AAAA;;AACA,MAAA,WAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,EAAA;AACA,OAHA,EAGA,IAHA,CAAA;AAIA;AA9CA,GAjEA;AAkHA,EAAA,QAAA,EAAA;AAlHA,CAAA","sourcesContent":["<template>\n<div>\n    <div style=\"text-align:center; color:#93b1ff; font-size:22px\">\n\n    Update the business information\n    </div>\n    \n\n    <div class=\"big-box\" >\n        \n        <div class=\"left\">\n        \n        <div class=\"left-inside\">\n            <div style=\"color: #F5C19E; font-size:24px;text-align:left; line-height:1em;\">\n            Business information\n        </div>\n        <tr>Business: {{ businessInfo[0].businessName }}</tr>\n        <tr>Address: </tr>\n        <tr>Street: {{ businessInfo[0].street }}</tr>\n        <tr>City:{{ businessInfo[0].city }}</tr>\n        <tr>State:{{businessInfo[0].state}} </tr>\n        <tr>Zip:0{{businessInfo[0].zip}}</tr>\n        </div>\n     \n        <div class=\"left-bottom\">\n         <div style=\"color: #F5C19E; font-size:24px;text-align:left; line-height:1em;\">\n            Operation Status\n        </div>   \n        <tr>Open: <input type=\"CheckBox\" id = \"open\" name=\"open\" value=true checked/> </tr>\n        Time From:<BusiInfoTimeDrop :initial_time=\"range.start\" /> To <BusiInfoTimeDropTo :initial_time=\"range.end\"/>\n        Day From:<BusiInfoDayDrop :initial_time=\"range.dayStart\"/> To <BusiInfoDayDropTo :initial_time=\"range.dayEnd\"/>\n\n        </div>\n        \n        </div>\n        \n        <div class=\"middel\">\n            <div class=\"left-inside\">\n                    <div style=\"color: #F5C19E; font-size:24px;text-align:left; line-height:1em;\">\n        Restaurants Facility:</div>\n        <tr>Takeout: <input type=\"CheckBox\" id = \"takeout\" name=\"takeout\" value=true checked/></tr>\n        <tr>Delivery: <input type=\"CheckBox\" id = \"delivery\" name=\"delivery\" value=true checked/> </tr>\n        <tr>Outdoor Sitting: <input type=\"CheckBox\" id = \"outdoor\" name=\"outdoor\" value=true checked/> </tr>\n        <tr>Indoor Sitting: <input type=\"CheckBox\" id = \"indoor\" name=\"indoor\" value=true checked/> </tr>\n        <tr>Parking: <input type=\"CheckBox\" id = \"parking\" name=\"parking\" value=true checked/></tr>\n\n        </div>\n         <div class=\"left-bottom\">\n        <div style=\"color: #F5C19E; font-size:24px;text-align:left; line-height:1em;\">\n        Covid Prevention:</div>\n        <tr>Safety Precaution: <input type=\"CheckBox\" id = \"safety\" name=\"safety\" value=true checked/></tr>\n\n        <tr>Mask: <input type=\"CheckBox\" id =\"mask\" name=\"mask\" value=true checked/></tr>\n\n        <tr>No Outdoor Shopping: <input type=\"CheckBox\" id =\"outdoorShopping\" name=\"outdoorShopping\" value=true checked/> </tr>\n        <tr>Curbside Pick Up: <input type=\"CheckBox\" id =\"curbside\" name=\"curbside\" value=true checked/> </tr>\n                <tr>Capacity: <input type=\"text\" id = \"capacity\" v-model=\"capacityInfo\"></tr>\n\n        </div> \n</div>\n        \n        <div class=\"right\">\n            <div class=\"left-inside\">\n                <div style=\"color: #F5C19E; font-size:24px;text-align:left; line-height:1em;\">\n        <tr>Description:</tr> </div>\n        <input type=\"text\" style=\"width:250px;height:300px\" v-model=\"announceInfo\">\n\n        </div> \n        <div class=\"left-bottom\">\n        <button class = \"cancel\" v-on:click=\"submit\">Submit</button>\n        <div v-if='success' class=\"success-message\">\n        {{ success }}\n      </div>\n        <!-- <button class = \"cancel\" v-on:click=\"cancel\">Cancel</button> -->\n        </div> \n        <!-- <input type=\"checkbox\" id=\"item.message\" v-model=\"item.checked\">  -->\n</div> \n\n\n\n    </div>\n    </div>\n\n\n</template>\n\n\n<script>\n// import { response } from 'express';\nimport axios from \"axios\";\nimport BusiInfoTimeDrop from \"./BusiInfoTimeDrop.vue\";\nimport BusiInfoTimeDropTo from \"./BusiInfoTimeDropTo.vue\";\nimport BusiInfoDayDrop from \"./BusiInfoDayDrop.vue\";\nimport BusiInfoDayDropTo from \"./BusiInfoDayDropTo.vue\";\nimport { eventBus } from \"../main\";\n\n\nexport default {\n  name: \"BusiInfoList\",\n\n    components: {  BusiInfoTimeDrop,BusiInfoTimeDropTo,  BusiInfoDayDrop,BusiInfoDayDropTo },\n\n  props: {\n\n  },\n  data() {\n      \n      return {\n          error: \"\",\n            success: \"\",\n          hourF:{selected:\"10:00:00 AM\"},\n          hourT:{selected:\"12:30:00 PM\"},\n          dayF:{selected:\"Sunday\"},\n          dayT:{selected:\"Saturday\"},\n          open:false,\n          capacityInfo:\"\",\n          announceInfo:\"\",\n          businessInfo :\"\",\n          range: {\n                date: \"11/09/2020\",\n                start: \"10:00:00 AM\",\n                end: \"12:30:00 PM\",\n                dayStart: \"Sunday\",\n                dayEnd:\"Saturday\",\n            }\n      }\n  },\n  created: function() {\n      eventBus.$on(\"business-time-selected\", (res) => {\n    \n        this.hourF = res;\n        \n\n      });\n      eventBus.$on(\"business-time-selected-to\", (res) => {\n    \n        this.hourT = res;\n       \n\n      });\n      eventBus.$on(\"busi-day-selected\", (res) => {\n    \n        this.dayF = res;\n       \n\n      });\n      eventBus.$on(\"busi-day-selected-to\", (res) => {\n    \n        this.dayT= res;\n       \n\n      });\n      eventBus.$on(\"update-success\", () => {\n/* eslint-disable no-console */\n              console.log(\"qqqqq\");\n      this.success = `Successfully update the information!`;\n      this.clearMessages();\n    });\n  },\n  mounted: function() {\n      this.loadBusiInfo();\n  },\n  methods: {\n      loadBusiInfo: function() {\n          axios.get(`/api/business/single`).then((response) => {\n                /* eslint-disable no-console */\n              console.log(response.data);\n              var singleBusinessInfo = response.data;\n              var authenticated = true; // need update\n                \n\n              if (authenticated === true) {\n                  \n                  this.businessInfo = singleBusinessInfo;\n\n              }\n\n          })\n      },\n      submit: function() {\n          const bodyContent = { os: document.getElementById(\"open\").checked, to: document.getElementById(\"takeout\").checked,\n                                od:document.getElementById(\"outdoor\").checked,ind:document.getElementById(\"indoor\").checked,\n                                inshop:document.getElementById(\"outdoorShopping\").checked,dl:document.getElementById(\"delivery\").checked,\n                                cs:document.getElementById(\"curbside\").checked,st:document.getElementById(\"safety\").checked,\n                                pk:document.getElementById(\"parking\").checked,mask:document.getElementById(\"mask\").checked,\n                                lat:null, lng:null, fb:null, ins:null, twitter:null, capacity:this.capacityInfo,\n                                hourFrom:this.hourF.selected, hourTo:this.hourT.selected, dayFrom:this.dayF.selected, dayTo:this.dayT.selected,\n                                des:this.announceInfo};\n          \n          axios.put(`/api/business/update`, bodyContent).then((response) => {\n            /* eslint-disable no-console */\n              console.log(response);\n              console.log(\"qqqqqqq\");\n              eventBus.$emit(\"update-success\", true);\n          }).catch(() => {\n              /* eslint-disable no-console */\n              console.log(\"qqqqqqq\");\n              eventBus.$emit(\"update-success\", true);\n              /* eslint-disable no-console */\n              console.log(\"qqqqqqq\");\n          })\n\n      },\n      clearMessages: function() {\n      setInterval(() => {\n        this.success = \"\";\n        this.error = \"\";\n      }, 5000);\n    }\n      \n  },\n  computed:{\n  }\n\n\n};\n</script>"],"sourceRoot":"src/components"}]}