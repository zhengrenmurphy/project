ainer\">\n   <h3> {{res.name}}</h3>\n   <div class= \"res-date-container\" v-for=\"date in res.times\" :key=\"date\">\n     {{new Intl.DateTimeFormat('en-US', options).format(date) }}\n      <button v-on:click=\"cancelReservation\">Cancel</button>\n   </div>\n   <br>\n  </div>\n</template>\n\n<script>\n\nimport axios from \"axios\";\nimport { eventBus } from \"../main\";\nexport default {\n  name: \"ResItem\",\n  props:{\n    res: Object\n  },\n  data() {\n    return {\n      options :{\n      year: 'numeric',\n      month: 'numeric',\n      day: 'numeric',\n      hour: 'numeric',\n      minute: 'numeric',\n      second: 'numeric'\n      }\n    };\n  },\n  methods:{\n    cancelReservation: function(){\n      axios.delete(`/api/client/reservation/${this.res}`, {})\n      .then((res) => {\n          // handle success\n          eventBus.$emit(\"cancel-reservation-success\", res);\n        })\n        .catch(err => {\n          // handle error\n          eventBus.$emit(\"cancel-reservation-error\", err);\n        })\n        .then(this.$router.go(0));\n    }\n  }\n};\n</script>\n\n<style scoped>\nh1{\n    color: #f5c19e\n}\nbutton{\n  background-color: #93b1ff;\n  color: white;\n  border-radius: 4px;\n  border: none;\n  padding: 4px;\n  margin-left: 20px;\n}\n.res-date-container{\n    display: flex;\n    margin-bottom: 10px;\n}\n</style>"],"sourceRoot":"src/components"}]}