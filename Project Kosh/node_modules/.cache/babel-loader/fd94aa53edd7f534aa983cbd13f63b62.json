MA,GAlBA;AAoBA,EAAA,OApBA,qBAoBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBACA,MAAA,CAAA,eAAA,EADA;;AAAA;AACA,cAAA,MAAA,CAAA,YADA;AAEA,cAAA,MAAA,CAAA,MAAA,GAAA,MAAA,CAAA,YAAA;;AACA,kBAAA,MAAA,CAAA,YAAA,CAAA,MAAA,EAAA;AACA,gBAAA,MAAA,CAAA,UAAA,GAAA,MAAA,CAAA,YAAA,CAAA,GAAA,CAAA,UAAA,GAAA;AAAA,yBAAA,GAAA,CAAA,YAAA;AAAA,iBAAA,CAAA;AACA;;AALA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAMA,GA1BA;AA4BA,EAAA,OAAA,EAAA;AACA,IAAA,eAAA,EAAA,2BAAA;AACA;AACA,aAAA,KAAA,CAAA,GAAA,mBACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,eAAA,GAAA,CAAA,IAAA;AACA,OAHA,CAAA;AAIA,KAPA;AAQA,IAAA,QAAA,EAAA,oBAAA;AAAA;;AACA,UAAA,KAAA,OAAA,KAAA,kBAAA,EAAA;AACA,aAAA,YAAA,GAAA,KAAA,MAAA;AACA,OAFA,MAEA;AACA,aAAA,YAAA,GAAA,KAAA,MAAA,CAAA,MAAA,CAAA,UAAA,GAAA;AAAA,iBAAA,GAAA,CAAA,YAAA,KAAA,MAAA,CAAA,OAAA;AAAA,SAAA,CAAA;AACA;AAEA;AAfA;AA5BA,CAAA","sourcesContent":["<template>\n    <div class=\"all-res-container\">\n        <h1> Your Reservations: </h1>\n        <div class=\"res-item-list\">\n            <div v-if=\"reservations.length\">\n              Find a reservation: \n              <select class = \"select\" style=\"width: 200px; height: 20px;\" v-model=\"resName\" @change='getValue'>\n                <option selected v-bind:key=\"allRes\"> All Reservations</option>\n                <option v-for='item in optionList' \n                v-bind:key=\"item\">{{ item }}</option>\n              </select>\n              <ResItem\n                  v-for=\"res in reservations\"\n                  v-bind:key=\"res.businessName\"\n                  v-bind:res=\"res\"\n              />\n            </div>\n            <div v-else>\n\n              <p style=\"text-align: center;\">You don't have any reservations. Make one below!</p>\n\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\n\nimport axios from \"axios\";\nimport ResItem from \"./ResItem.vue\";\nimport { eventBus } from \"../main\";\n\nexport default {\n  name: \"AllResClients\",\n  components: {ResItem},\n  data() {\n    return {\n      reservations: [],\n      allRes: [],\n      optionList: [],\n      success: '',\n      resName: ''\n    };\n  },\n  async created(){\n    eventBus.$on(\"delete-res\", async res => {\n      this.reservations = await this.getReservations()\n      this.allRes = this.reservations\n      this.success = res.data.message\n    });\n  },\n\n  async mounted(){\n    this.reservations = await this.getReservations()\n    this.allRes = this.reservations\n    if(this.reservations.length){\n      this.optionList = this.reservations.map(res => res.businessName)\n    }\n  },\n\n  methods: {\n    getReservations: function(){\n      // TODO check authentication\n      return axios.get(`/api/clientRes`)\n      .then((res) => {\n        return res.data\n      })\n    },\n    getValue: function(){\n      if(this.resName === 'All Reservations'){\n        this.reservations = this.allRes\n      }else{\n        this.reservations = this.allRes.filter(res => res.businessName === this.resName)\n      }\n\n    }\n  }\n};\n</script>\n\n<style scoped>\nh1{\n    color: #f5c19e\n}\n</style>"],"sourceRoot":"src/components"}]}