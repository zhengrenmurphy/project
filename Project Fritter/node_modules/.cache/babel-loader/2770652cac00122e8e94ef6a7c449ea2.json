AA,IAAA,CAAA,SAAA,8BACA,GAAA,CAAA,IADA;;AAGA,MAAA,KAAA,CAAA,aAAA;;AACA,MAAA,KAAA,CAAA,UAAA;AACA,KANA;AAQA,IAAA,QAAA,CAAA,GAAA,CAAA,sBAAA,EAAA,UAAA,GAAA,EAAA;AACA,MAAA,KAAA,CAAA,OAAA,wBAAA,GAAA,CAAA,SAAA;;AACA,MAAA,KAAA,CAAA,aAAA;;AACA,MAAA,KAAA,CAAA,UAAA;AACA,KAJA;AAMA,IAAA,QAAA,CAAA,GAAA,CAAA,oBAAA,EAAA,UAAA,GAAA,EAAA;AACA,MAAA,KAAA,CAAA,KAAA,kCAAA,GAAA,CAAA,IAAA,CAAA,SAAA;;AACA,MAAA,KAAA,CAAA,aAAA;;AACA,MAAA,KAAA,CAAA,UAAA;AACA,KAJA;AAMA,IAAA,QAAA,CAAA,GAAA,CAAA,oBAAA,EAAA,UAAA,GAAA,EAAA;AACA,MAAA,KAAA,CAAA,KAAA,kCAAA,GAAA,CAAA,SAAA;;AACA,MAAA,KAAA,CAAA,aAAA;;AACA,MAAA,KAAA,CAAA,UAAA;AACA,KAJA;AAKA,GAvDA;AAyDA,EAAA,OAAA,EAAA,mBAAA;AACA,SAAA,UAAA;AACA,GA3DA;AA6DA,EAAA,OAAA,EAAA;AACA,IAAA,UAAA,EAAA,sBAAA;AAAA;;AACA,UAAA,IAAA,GAAA;AAAA,QAAA,EAAA,EAAA,KAAA,OAAA,CAAA,GAAA,CAAA,cAAA;AAAA,OAAA;AAEA,MAAA,KAAA,CACA,GADA,8BACA,IAAA,CAAA,EADA,GACA,IADA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,QAAA,CAAA,IAAA;AACA,OAHA,EAHA,CAOA;AACA;AACA;AACA;AACA,KAZA;AAcA,IAAA,aAAA,EAAA,yBAAA;AAAA;;AACA,MAAA,WAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,EAAA;AACA,OAHA,EAGA,IAHA,CAAA;AAIA;AAnBA;AA7DA,CAAA","sourcesContent":["<template>\n  <div class=\"subcontainer\">\n    <div class=\"header secondary-header\">\n      Freets\n    </div>\n    <div>\n      <div v-if='success' class=\"success-message\">\n        {{ success }}\n      </div>\n      <div v-if='error' class=\"error-message\">\n        {{ error }}\n      </div>\n\n      <div class=\"freets-container\">  \n        <div v-if=\"freets.length\">\n          <Freet\n          v-for=\"freet in freets\"\n          v-bind:key=\"freet.id\"\n          v-bind:freet=\"freet\"\n        />\n        </div>\n        <div v-else>\n          <p style=\"text-align: center;\">There are no freets to display. Create one today!</p>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n// import FreetListItem from \"./FreetListItem\";\nimport Freet from \"./Freet\";\n\n\nimport { eventBus } from \"../main\";\n\nexport default {\n  name: \"FreetList\",\n\n  components: { Freet },\n\n  props: {\n      user : Object,\n      isSignedIn:Boolean,\n    },\n\n  data() {\n    return {\n      error: \"\",\n      success: \"\",\n      freets: [],\n    };\n  },\n\n  created: function() {\n    // PROBLEM HERE!\n    eventBus.$once(\"create-freet-success\", res => {\n      this.freets.push(res.data);\n      this.loadFreets();\n    });\n\n    eventBus.$once(\"create-refreet-success\", res => {\n      this.freets.push(res.data);\n      this.loadFreets();\n    });\n\n    eventBus.$on(\"update-freet-success\", res => {\n      this.success = `Freet name ${res.data.freetName} now resolves to ${\n        res.data\n      }`;\n      this.clearMessages();\n      this.loadFreets();\n    });\n\n    eventBus.$on(\"delete-freet-success\", res => {\n      this.success = `Freet name ${res.freetName} has been deleted`;\n      this.clearMessages();\n      this.loadFreets();\n    });\n\n    eventBus.$on(\"update-freet-error\", res => {\n      this.error = `Error updating freet ${res.data.freetName}`;\n      this.clearMessages();\n      this.loadFreets();\n    });\n\n    eventBus.$on(\"delete-freet-error\", res => {\n      this.error = `Error deleting freet ${res.freetName}`;\n      this.clearMessages();\n      this.loadFreets();\n    });\n  },\n\n  mounted: function() {\n    this.loadFreets();\n  },\n\n  methods: {\n    loadFreets: function() {\n      const body = { id : this.$cookie.get('fritter-auth')};\n\n      axios\n      .get(`/api/freets?author=${body.id}`, body).then(response => {\n        this.freets = response.data;\n      })\n      // .then((res) => {\n      //     // handle success\n      //     eventBus.$emit(\"update-short-success\", res);\n      //   });\n    },\n\n    clearMessages: function() {\n      setInterval(() => {\n        this.success = \"\";\n        this.error = \"\";\n      }, 5000);\n    }\n  }\n};\n</script>\n\n"],"sourceRoot":"src/components"}]}