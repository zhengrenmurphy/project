A,CAAA,IAAA,mDAAA,QAAA;;AACA,MAAA,KAAA,CAAA,aAAA;AACA,KAHA;AAKA,IAAA,QAAA,CAAA,GAAA,CAAA,kBAAA,EAAA,UAAA,QAAA,EAAA;AACA,MAAA,KAAA,CAAA,QAAA,CAAA,IAAA,qDAAA,QAAA;;AACA,MAAA,KAAA,CAAA,aAAA;AACA,KAHA;AAKA,GArCA;AAsCA,EAAA,OAAA,EAAA;AACA,IAAA,SADA,qBACA,QADA,EACA;AAAA;;AACA,MAAA,KAAA,CAAA,GAAA,sBAAA,QAAA,GAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA,CADA,CAEA;AACA,OAHA;AAIA,KANA;AAQA,IAAA,aAAA,EAAA,yBAAA;AAAA;;AACA,MAAA,WAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,EAAA;AACA,OAFA,EAEA,IAFA,CAAA;AAIA;AAbA;AAtCA,CAAA","sourcesContent":["<template>\n  <div>\n\n   <OtherUserProfileHeader\n      :user=\"user\"\n    />\n\n    <div class=\"ui stackable grid container\">\n      <div class=\"four wide column\">\n        <OtherUserProfileSidebarDetails :user=\"user\"/>\n      </div>\n      <div class=\"eight wide column\">\n        <div class=\"ui segment\">\n          <h2 class=\"ui medium dividing header\">Freets</h2>\n\n      </div>\n\n      </div>\n     <!--  <div class=\"four wide column\">\n        <WhoToFollow v-if=\"isLoggedIn\"/>\n      </div> -->\n    </div>\n    <div v-if='messages.length' class=\"success-message\" style=\"text-align:center;\">\n      <div v-for='message in messages' v-bind:key='message.id'>{{ message }}</div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport { eventBus } from \"../../main\";\n\nimport OtherUserProfileHeader from './OtherUserProfileHeader'\nimport OtherUserProfileSidebarDetails from './OtherUserProfileSidebarDetails'\n// import WhoToFollow from '@/components/User/WhoToFollow'\n// import Tweets from '@/components/Tweet/Tweets'\n\n  export default {\n    name: 'OtherUserProfile',\n    components: {\n      OtherUserProfileSidebarDetails,\n      OtherUserProfileHeader,\n      // WhoToFollow,\n      // Tweets\n    },\n    data () {\n      return {\n        user: {},\n        messages: []\n        //authUser: '',\n        //tweets: []\n      }\n    },\n    // beforeRouteUpdate (to, from, next) {\n    //   this.fetchUser(to.params.username);\n\n    //   next();\n    // },\n    computed: {\n\n    },\n    created () {\n      this.fetchUser(this.$route.params.username);\n\n      eventBus.$on(\"follow-success\", (username) => {\n      this.messages.push(`You have successfuly followed this user ${username}!`);\n      this.clearMessages();\n    });\n\n      eventBus.$on(\"unfollow-success\", (username) => {\n      this.messages.push(`You have successfuly unfollowed this user ${username}!`);\n      this.clearMessages();\n    });\n\n    },\n    methods: {\n      fetchUser (username) {\n        axios.get(`/api/users/${username}`).then(response => {\n          this.user = response.data.user;\n          // this.tweets = response.data.data.tweets.reverse()\n        })\n      },\n\n      clearMessages: function() {\n      setInterval(() => {\n        this.messages = [];\n      }, 5000);\n      \n      },\n      \n    }\n  }\n</script>"],"sourceRoot":"src/components/OtherUser"}]}