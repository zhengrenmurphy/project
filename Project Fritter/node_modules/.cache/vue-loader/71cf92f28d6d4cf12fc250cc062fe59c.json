e }}</div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport { eventBus } from \"../../main\";\n\nimport OtherUserProfileHeader from './OtherUserProfileHeader'\nimport OtherUserProfileSidebarDetails from './OtherUserProfileSidebarDetails'\n// import WhoToFollow from '@/components/User/WhoToFollow'\n// import Tweets from '@/components/Tweet/Tweets'\n\n  export default {\n    name: 'OtherUserProfile',\n    components: {\n      OtherUserProfileSidebarDetails,\n      OtherUserProfileHeader,\n      // WhoToFollow,\n      // Tweets\n    },\n    data () {\n      return {\n        user: {},\n        messages: []\n        //authUser: '',\n        //tweets: []\n      }\n    },\n    // beforeRouteUpdate (to, from, next) {\n    //   this.fetchUser(to.params.username);\n\n    //   next();\n    // },\n    computed: {\n\n    },\n    created () {\n      this.fetchUser(this.$route.params.username);\n\n      eventBus.$on(\"follow-success\", (username) => {\n      this.messages.push(`You have successfuly followed this user ${username}!`);\n      this.clearMessages();\n    });\n\n      eventBus.$on(\"unfollow-success\", (username) => {\n      this.messages.push(`You have successfuly unfollowed this user ${username}!`);\n      this.clearMessages();\n    });\n\n    },\n    methods: {\n      fetchUser (username) {\n        axios.get(`/api/users/${username}`).then(response => {\n          this.user = response.data.user;\n          // this.tweets = response.data.data.tweets.reverse()\n        })\n      },\n\n      clearMessages: function() {\n      setInterval(() => {\n        this.messages = [];\n      }, 5000);\n      \n      },\n      \n    }\n  }\n</script>"]}]}